{
  "name": "react-layout-kit",
  "version": "1.6.1",
  "description": "a npm develop template",
  "keywords": [
    "react",
    "layout",
    "flexbox"
  ],
  "homepage": "https://github.com/arvinxx/react-layout-kit",
  "bugs": {
    "url": "https://github.com/arvinxx/react-layout-kit/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arvinxx/react-layout-kit.git"
  },
  "license": "MIT",
  "author": "arvinxx(arvinx@foxmail.com)",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "ci": "npm run lint && npm run doctor && npm run type-check",
    "clean": "rm -rf es lib dist coverage .dumi/tmp .eslintcache",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && npm run setup",
    "prepublishOnly": "father doctor && npm run build",
    "prettier": "prettier -c --write \"**/**\"",
    "release": "semantic-release",
    "setup": "dumi setup",
    "start": "dumi dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest -u",
    "type-check": "tsc -p tsconfig-check.json"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@emotion/css": "^11.10.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@emotion/jest": "^11.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@umijs/lint": "^4.0.64",
    "@vitest/coverage-c8": "latest",
    "antd": "^5.4.0",
    "commitlint": "^17.5.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dumi": "^2.1.19",
    "dumi-theme-antd-style": "latest",
    "eslint": "^8.38.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "father": "^4.1.8",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^19.0.5",
    "semantic-release-config-gitmoji": "^1.4.3",
    "stylelint": "^14.16.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "latest"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
